  },
  "dependencies": {
    "markdown-it": "^14.1.0",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.3",
    "prettier": "3.8.1",
    "vite": "^7.3.1",
    "vite-plugin-vue-devtools": "^8.0.5",
    "wrangler": "^4.60.0"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}
