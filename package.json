{
  "name": "blog",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node scripts/generate-og-png.mjs && node node_modules/vite/bin/vite.js",
    "build": "node scripts/generate-og-png.mjs && node node_modules/vite/bin/vite.js build --ssrManifest && node node_modules/vite/bin/vite.js build --ssr src/entry-server.js --outDir dist-ssr && node scripts/ssg.mjs",
